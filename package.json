{
  "name": "safety-lens",
  "version": "1.3.0",
  "description": "Type-safe, functional lens library supporting Immutable.js",
  "author": "Jesse Hallett <jesse@sitr.us>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/hallettj/lens.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hallettj/lens.js.git"
  },
  "main": "lens.js",
  "scripts": {
    "test": "node_modules/.bin/babel-node node_modules/.bin/_mocha --full-trace --recursive -R spec -C --check-leaks test/",
    "typecheck": "node_modules/.bin/flow check",
    "prepublish": "npm run typecheck && make && npm test"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-flow-strip-types"
    ]
  },
  "keywords": [
    "lens",
    "lenses",
    "functional",
    "Flow",
    "cursors",
    "immutable",
    "Immutable.js"
  ],
  "files": [
    "lens.js",
    "lens.js.flow",
    "lens.js.map",
    "immutable.js",
    "immutable.js.flow",
    "immutable.js.map",
    "es2015.js",
    "es2015.js.flow",
    "es2015.js.map",
    "lib"
  ],
  "dependencies": {
    "immutable": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.3.0",
    "babel-plugin-syntax-flow": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.0.0",
    "chai-immutable": "^1.2.0",
    "deep-equal": "^1.0.1",
    "flow-bin": "^0.21.0",
    "jsverify": "^0.7.1",
    "mocha": "^2.2.5"
  }
}
